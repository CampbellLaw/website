{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "env": {
    "GTM_ID": "",
    "CRYSTALLIZE_GRAPH_URL_BASE": "https://graph.crystallize.com",
    "CRYSTALLIZE_TENANT_ID": "demo"
  },
  "routes": [
    {
      "src": "/api/magic-link/([^/]+)",
      "dest": "/api/magic-link?email=$1"
    },
    {
      "src": "/api/verify/([^/]+)",
      "dest": "/api/verify?token=$1"
    }
  ]
}
