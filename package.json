{
   "name": "crystallize-frontend-boilerplate",
   "version": "2.5.0",
   "private": true,
   "license": "MIT",
   "engines": {
      "node": ">=8.10"
   },
   "repository": {
      "url": "git@github.com:snowballdigital/crystallize-frontend-boilerplate.git",
      "type": "git"
   },
   "scripts": {
      "dev": "node server/index.js",
      "dev:inspect": "node --inspect-brk server/index.js",
      "build": "next build",
      "start": "NODE_ENV=production node server/index.js",
      "prod": "yarn build && yarn start",
      "test": "NODE_ENV=test jest",
      "lint": "eslint .",
      "validate": "yarn test && yarn lint-staged",
      "greet": "node .crystallize-greeting/",
      "postinstall": "yarn greet"
   },
   "husky": {
      "hooks": {
         "pre-commit": "yarn validate"
      }
   },
   "lint-staged": {
      "*.js": [
         "prettier --write",
         "eslint --fix",
         "git add"
      ]
   },
   "dependencies": {
      "@babel/core": "^7.1.2",
      "@crystallize/content-chunk": "^3.2.0",
      "@crystallize/node-klarna": "^1.4.0",
      "@crystallize/react-basket": "^4.0.1",
      "@crystallize/react-image": "^2.1.0",
      "@crystallize/react-layout": "^1.8.0",
      "apollo-boost": "^0.1.17",
      "apollo-client": "^2.4.3",
      "babel-plugin-module-resolver": "^3.1.1",
      "babel-plugin-styled-components": "^1.8.0",
      "body-parser": "^1.18.3",
      "change-case": "^3.0.2",
      "cross-fetch": "^2.2.2",
      "deep-assign": "^2.0.0",
      "dotenv": "^6.0.0",
      "express": "^4.16.3",
      "graphql": "^14.0.2",
      "graphql-tag": "^2.9.2",
      "helmet": "^3.13.0",
      "i18next": "^11.7.0",
      "isarray": "^2.0.4",
      "next": "^7.0.2",
      "ow": "^0.8.0",
      "polished": "^2.3.0",
      "prop-types": "^15.6.2",
      "react": "^16.6.0",
      "react-apollo": "^2.2.4",
      "react-dom": "^16.6.0",
      "react-i18next": "^7.11.1",
      "styled-components": "^4.0.0-beta.10",
      "styled-is": "^1.1.5",
      "upper-case-first": "^1.1.2"
   },
   "devDependencies": {
      "babel-core": "7.0.0-bridge.0",
      "babel-eslint": "^10.0.1",
      "babel-jest": "^23.6.0",
      "chalk": "^2.4.1",
      "enzyme": "^3.7.0",
      "enzyme-adapter-react-16": "^1.5.0",
      "eslint": "^5.6.0",
      "eslint-config-airbnb": "^17.1.0",
      "eslint-config-prettier": "^3.1.0",
      "eslint-import-resolver-babel-module": "^5.0.0-beta.1",
      "eslint-plugin-import": "^2.14.0",
      "eslint-plugin-jsx-a11y": "^6.1.1",
      "eslint-plugin-prettier": "^2.6.2",
      "eslint-plugin-react": "^7.11.1",
      "husky": "^1.1.2",
      "inquirer": "^6.2.0",
      "jest": "^23.6.0",
      "lint-staged": "^7.3.0",
      "node-emoji": "^1.8.1",
      "prettier": "^1.14.3",
      "supports-color": "^5.5.0"
   }
}
