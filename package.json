{
  "name": "crystallize-frontend-boilerplate",
  "version": "3.1.0",
  "license": "MIT",
  "engines": {
    "node": ">=8.10"
  },
  "repository": {
    "url": "git@github.com:CrystallizeAPI/crystallize-frontend-boilerplate.git",
    "type": "git"
  },
  "scripts": {
    "dev": "node ./index.js",
    "build": "next build",
    "start": "NODE_ENV=production node ./index.js",
    "prod": "npm run build && npm run start",
    "lint": "eslint .",
    "validate": "lint-staged",
    "greet": "node .crystallize-greeting/",
    "postinstall": "npm run greet"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run validate"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@crystallize/content-chunk": "^3.3.0",
    "@crystallize/node-klarna": "^2.1.1",
    "@crystallize/react-image": "^3.0.0-beta.7",
    "@crystallize/react-layout": "^3.0.0",
    "apollo-boost": "^0.3.1",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-styled-components": "^1.10.0",
    "body-parser": "^1.19.0",
    "change-case": "^3.1.0",
    "cross-fetch": "^3.0.2",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "graphql": "^14.3.0",
    "graphql-tag": "^2.10.1",
    "helmet": "^3.18.0",
    "jsonwebtoken": "^8.5.1",
    "mjml": "^4.4.0",
    "next": "^9.0.3-canary.4",
    "next-i18next": "^0.51.0",
    "ow": "^0.8.0",
    "polished": "^3.3.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.5",
    "react-dom": "^16.8.6",
    "react-intl": "^3.1.5",
    "styled-components": "^5.0.0-beta.8",
    "styled-is": "^1.3.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "chalk": "^2.4.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.2.0",
    "inquirer": "^6.2.0",
    "lint-staged": "^8.1.6",
    "node-emoji": "^1.8.1",
    "prettier": "^1.18.2",
    "supports-color": "^6.1.0"
  }
}
